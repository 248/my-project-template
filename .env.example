# プロジェクト設定
PROJECT_NAME=my-project-template
NODE_ENV=development

# API設定
NEXT_PUBLIC_API_BASE_URL=http://localhost:8080

# データベース設定
DATABASE_URL=postgresql://template_user:template_pass@localhost:5432/template_db
POSTGRES_DB=template_db
POSTGRES_USER=template_user
POSTGRES_PASSWORD=template_pass

# Clerk Authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key
CLERK_SECRET_KEY=your_clerk_secret_key

# Clerk JWT設定（バックエンド用）
CLERK_JWT_ISSUER=https://your-domain.clerk.accounts.dev
# CLERK_JWT_AUD=backend

# Redis設定
REDIS_URL=redis://localhost:6379

# PgAdmin設定
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin
PGADMIN_CONFIG_SERVER_MODE=False
PGADMIN_CONFIG_MASTER_PASSWORD_REQUIRED=False