{
  "name": "@template/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0 --cache --ignore-path ../../.eslintignore",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --cache",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality-check": "npm run type-check && npm run lint && npm run format:check",
    "clean": "rm -rf .next dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "echo 'Frontend setup completed'"
  },
  "dependencies": {
    "@template/api-contracts-ts": "workspace:*",
    "@template/shared": "workspace:*",
    "@template/ui": "workspace:*",
    "@clerk/nextjs": "^6.31.5",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.15.0",
    "date-fns": "^4.1.0",
    "next": "15.5.2",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "svix": "^1.75.0",
    "swagger-ui-react": "^5.27.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@template/config": "workspace:*",
    "@apidevtools/swagger-parser": "^10.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.8.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/swagger-ui-react": "^5.18.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.2",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.20.5"
  }
}
